x-ec2_anchor: &ec2_anchor
  associateIPStrategy: PUBLIC_IP
  connectBySSHProcess: false
  connectionStrategy: PUBLIC_IP
  deleteRootOnTermination: false
  ebsOptimized: false
  idleTerminationMinutes: "10"
  maxTotalUses: -1
  monitoring: false
  numExecutors: 4
  stopOnTerminate: false
  tenancy: Default
  useEphemeralDevices: false
  zone: "{{ jenkins_master_ec2_region }}"
  ami: "{{ jenkins_master_ec2_ami }}"
  securityGroups: "{{ jenkins_master_ec2_security_group }}"
  subnetId: "{{ jenkins_master_ec2_subnet_id }}"
  remoteFS: "/home/admin"
  remoteAdmin: "admin"
  mode: "EXCLUSIVE"
  amiType:
    unixData:
      sshPort: "22"

configuration-as-code: {
  }
credentials:
  folder: {
    }
  providerImpl: {
    }
  system:
    domainCredentials:
    - credentials:
      - basicSSHUserPrivateKey:
          description: "SSH key credential"
          id: "ssh-key-credential-id"
          privateKeySource:
            directEntry:
              privateKey: "${readFile:{{ __jenkins_master_sshkey_file }}}"
          username: "admin"
      domain: {
        }
  user: {
    }
jenkins:
  authorizationStrategy:
    loggedInUsersCanDoAnything:
      allowAnonymousRead: false
  clouds:
    - amazonEC2:
        name: "ec2"
        instanceCapStr: 10
        useInstanceProfileForCredentials: true
        sshKeysCredentialsId: "{{ __jenkins_master_sshkey_credential_id }}"
        noDelayProvisioning: true
        region: "{{ jenkins_master_ec2_region }}"
        templates:
{% for distri in jenkins_master_ec2_ami_distributions %}
          - description: "jenkins-debian-glue-slave {{ distri }}"
            type: "t3.large"
            labelString: "slave:{{ distri }}"
            initScript: |
              sudo sed -i 's?#:APT-CACHER:#?export http_proxy=http://{{ jenkins_master_apt_cache_servername }}?' /etc/jenkins/pbuilderrc
              sudo /home/admin/prepare_cowbuilder.sh {{ distri }} $(dpkg --print-architecture) 2>&1 | tee /tmp/initsh.log
            <<: *ec2_anchor
{% endfor %}
  crumbIssuer:
    standard:
      excludeClientIPFromCrumb: false
  disableRememberMe: false
  labelAtoms:
  - name: "built-in"
  log: {
    }
  markupFormatter: "plainText"
  mode: NORMAL
  nodeMonitors:
  - "architecture"
  - "clock"
  - diskSpace:
      freeSpaceThreshold: "1GiB"
      freeSpaceWarningThreshold: "2GiB"
  - "swapSpace"
  - tmpSpace:
      freeSpaceThreshold: "1GiB"
      freeSpaceWarningThreshold: "2GiB"
  - "responseTime"
  numExecutors: {{ jenkins_master_executors }}
  primaryView:
    all:
      name: "all"
  projectNamingStrategy: "standard"
  quietPeriod: 5
  scmCheckoutRetryCount: 0
  securityRealm:
    local:
      allowsSignup: false
      enableCaptcha: false
      users:
      - id: "admin"
        name: "admin"
        properties:
        - "mailer"
        - preferredProvider:
            providerId: "default"
        - "apiToken"
        - "consoleUrlProvider"
        - "viewsTabBar"
        - myView:
            viewsTabBar: "standard"
        - "timezone"
        - "experimentalFlags"
  slaveAgentPort: -1
  updateCenter:
    sites:
    - id: "default"
      url: "https://updates.jenkins.io/update-center.json"
  views:
  - all:
      name: "all"
  viewsTabBar: "standard"
globalCredentialsConfiguration:
  configuration:
    providerFilter: "none"
    typeFilter: "none"
appearance:
  consoleUrlProvider: {
    }
  prism:
    theme: PRISM
security:
  apiToken:
    creationOfLegacyTokenEnabled: false
    tokenGenerationOnCreationEnabled: false
    usageStatisticsEnabled: true
  apiTokenProperty: {
    }
  copyartifact:
    mode: PRODUCTION
  cps:
    hideSandbox: false
  crumb: {
    }
  gitHooks:
    allowedOnAgents: false
    allowedOnController: false
  gitHostKeyVerificationConfiguration:
    sshHostKeyVerificationStrategy: "knownHostsFileVerificationStrategy"
  prism: {
    }
  queueItemAuthenticator: {
    }
  scriptApproval:
{% if jenkins_master_scriptapproval_approvedscripthashes|length > 0 %}
    approvedScriptHashes:
{%  for item in jenkins_master_scriptapproval_approvedscripthashes %}
    - "{{ item }}"
{%  endfor %}
{% else %}
    approvedScriptHashes: []
{% endif %}
    forceSandbox: false
  updateSiteWarningsConfiguration: {
    }
unclassified:
  administrativeMonitorsConfiguration: {
    }
  artifactManager: {
    }
  buildDiscarders:
    configuredBuildDiscarders:
    - "jobBuildDiscarder"
  casCGlobalConfig: {
    }
  computerRetentionCheckInterval: {
    }
  defaultDisplayUrlProvider: {
    }
  defaultFolderConfiguration: {
    }
  defaultView: {
    }
  email-ext:
    adminRequiredForTemplateTesting: false
    allowUnregisteredEnabled: false
    charset: "UTF-8"
    debugMode: false
    defaultBody: |-
      $PROJECT_NAME - Build # $BUILD_NUMBER - $BUILD_STATUS:

      Check console output at $BUILD_URL to view the results.
    defaultSubject: "$PROJECT_NAME - Build # $BUILD_NUMBER - $BUILD_STATUS!"
    defaultTriggerIds:
    - "hudson.plugins.emailext.plugins.trigger.FailureTrigger"
    mailAccount: {
      }
    maxAttachmentSize: -1
    maxAttachmentSizeMb: -1
    precedenceBulk: false
    throttlingEnabled: false
    watchingEnabled: false
  envVarsFilter: {
    }
  fingerprints:
    fingerprintCleanupDisabled: false
    storage: "file"
  globalDefaultFlowDurabilityLevel: {
    }
  globalLibraries: {
    }
  globalUntrustedLibraries: {
    }
  junitTestResultStorage:
    storage: "file"
  location:
    adminAddress: "{{ jenkins_master_admin_address }}"
    url: "https://{{ jenkins_master_servername }}/"
  mailer:
    charset: "UTF-8"
    useSsl: false
    useTls: false
  myView: {
    }
  nodeProperties: {
    }
  plugin: {
    }
  pollSCM:
    pollingThreadCount: 10
  projectNamingStrategy: {
    }
  proxyConfigurationManager: {
    }
  quietPeriod: {
    }
  resourceRoot: {
    }
  scmGit:
    addGitTagAction: false
    allowSecondFetch: false
    createAccountBasedOnEmail: false
    disableGitToolChooser: false
    hideCredentials: false
    showEntireCommitSummaryInChanges: false
    useExistingAccountWithSameEmail: false
  scmRetryCount: {
    }
  shell: {
    }
  upstream:
    globalUpstreamFilterStrategy: UseOldest
  usageStatistics: {
    }
  viewsTabBar: {
    }
tool:
  git:
    installations:
    - home: "git"
      name: "Default"
  mavenGlobalConfig:
    globalSettingsProvider: "standard"
    settingsProvider: "standard"
